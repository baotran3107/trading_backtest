{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Flutter App",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        {
            "name": "Launch Flutter App (Debug)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--debug"
            ],
            "flutterMode": "debug"
        },
        {
            "name": "Launch Flutter App (Profile)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--profile"
            ],
            "flutterMode": "profile"
        },
        {
            "name": "Launch Flutter App (Release)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--release"
            ],
            "flutterMode": "release"
        },
        {
            "name": "Launch Flutter Web",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "web-server",
                "--web-port",
                "3000"
            ],
            "deviceId": "web-server"
        },
        {
            "name": "Launch Flutter Chrome",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome"
            ],
            "deviceId": "chrome"
        },
        {
            "name": "Attach to Flutter Process",
            "type": "dart",
            "request": "attach"
        }
    ]
}